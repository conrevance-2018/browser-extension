{
  "short_name": "Europa",
  "name": "Jupiter Europa",
  "description": "Connect to Jupiter Blockchain Network",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "action": {
    "default_icon": "logo512.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "static/background.js"
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.jup.io/*"
    ]
  }
}