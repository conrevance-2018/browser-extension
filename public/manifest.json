{
  "short_name": "JupMask",
  "name": "JupMask",
  "description": "Connect to Jupiter Chain Network",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "static/background.js"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "activeTab",
    "webRequest",
    "notifications",
    "http://localhost/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}